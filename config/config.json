    "username": "${DEV_DB_USERNAME}",
    "password": "${DEV_DB_PASSWORD}",
    "database": "${DEV_DB_DATABASE}",
    "port":"${DEV_SERVER_PORT}",
    "host": "${DEV_SERVER_HOST}",
    "dialect": "${DEV_DB_DIALECT}"
  },
  "test": {
    "use_env_variable": "TEST_DATABASE_URL",
